    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Implant",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "buildFlags": "-tags implant",
            "program": "${workspaceFolder}/cmd/implant/main/"
        },
        {
            "name": "Launch Debug LP",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "buildFlags": "-tags lp",
            "program": "${workspaceFolder}/cmd/debugLp/main"
        },
        {
            "name": "Launch Current File",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${file}"
        }
    ]
}